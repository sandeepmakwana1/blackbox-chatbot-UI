{
	"compilerOptions": {
		"target": "ES2022",
		"lib": ["DOM", "DOM.Iterable", "ES2022"],
		"module": "ES2022",
		"moduleResolution": "bundler",
		"jsx": "react-jsx",
		"allowJs": false,
		"strict": false,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"resolveJsonModule": true,
		"isolatedModules": true,
		"types": ["vite/client"],
		"baseUrl": ".",
		"paths": {
			"~/*": ["./src/*"]
		}
	},
	"include": ["src", "vite.config.ts", "tsconfig.json"]
}
